#include <iostream>
#include <cmath>
using namespace std;

int main() {
  float num;

  cout << "Enter a float number: ";
  cin >> num;
  
  float floor_value = floor(num);
  float ceil_value = ceil(num);

  cout << "floor value: " << floor_value << endl;
  cout << "ceil value: " << ceil_value << endl;

  return 0;
}
